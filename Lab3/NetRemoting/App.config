<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
  </startup>

  <system.runtime.remoting>
    <application>
      <service>
        <wellknown mode="SingleCall"
                type="NetRemoting.DatabaseManangerServer, NetRemoting"
                objectUri="DatabaseMananger"/>
      </service>
      <channels>
        <channel ref="tcp" port="9990" />
        <channel ref="http" port="9999" />
      </channels>
    </application>
  </system.runtime.remoting>

</configuration>
